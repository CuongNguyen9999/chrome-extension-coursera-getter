{
  "manifest_version": 3,
  "name": "Coursera Get Shareable Link",
  "description": "Get shareable link Coursera Peer-review",
  "version": "1.0.0",
  "icons": {
    "128": "icon_128.png"
  },
  "action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "host_permissions": [
    "<all_urls>"
  ]
}
